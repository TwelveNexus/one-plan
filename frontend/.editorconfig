# EditorConfig is awesome: https://EditorConfig.org

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[{*.ts,*.tsx}]
indent_style = space
indent_size = 2


[{*.js,*.jsx}]
indent_style = space
indent_size = 2

[*.json]
indent_style = space
indent_size = 2
